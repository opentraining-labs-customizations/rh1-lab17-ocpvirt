apiVersion: v1
kind: Service
metadata:
  name: exercise2
  namespace: "namespace-{{ user }}"
spec:
  internalTrafficPolicy: Cluster
  ipFamilies:
    - IPv4
  ipFamilyPolicy: SingleStack
  ports:
    - port: 80
      protocol: TCP
      targetPort: 80
  selector:
    vm.kubevirt.io/name: exercise2
  sessionAffinity: None
  type: ClusterIP
